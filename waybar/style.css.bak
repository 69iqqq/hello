* {
  padding: 0;
  margin: 0;
  font-family: JetBrainsMono Nerd Font;
  font-weight: bold;
  font-size: 16px;
}

window#waybar {
  background-color: rgba(33, 35, 55, 0.9);
}

#window {
  margin-left: 5px;
  margin-top: 1px;
  background-color: rgba(0, 0, 0, 0);
}

#custom-launcher {
  font-size: 18px;
  color: #37f499;
  padding-right: 10px;
}

#cpu,
#memory,
#temperature {
  font-size: 15px;
  background: #2a2d45;
  margin-top: 5px;
  margin-bottom: 5px;
}

#cpu {
  color: #eed49f;
  padding: 0px 10px 0px 12px;
  border-radius: 5px 0px 0px 5px;
}

#memory {
  color: #a6da95;
  padding: 0px 10px 0px 0px;
}

#temperature {
  color: #8aadf4;
  padding: 0px 12px 0px 0px;
  border-radius: 0px 5px 5px 0px;
}

#custom-window-name {
  color: #a48cf2;
  margin-right: 10px;
  background-color: #212337;
}

