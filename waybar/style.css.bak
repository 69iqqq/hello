#custom-right-arr {
  color: #ebfafa;
  background-color: #212337;
}

#network {
  color: #a48cf2;
  margin-right: 10px;
  background-color: #212337;
}

#custom-dot {
  color: #6e738d;
  padding-right: 10px;
  background-color: #212337;
}

#custom-dot-alt {
  color: #a5adcb;
  padding-left: 10px;
  padding-right: 10px;
}

#disk {
  color: #a48cf2;
  background-color: #212337;
}

#custom-clock-icon {
  color: #ebfafa;
  padding-left: 10px;
  padding-right: 10px;
  margin-top: 5px;
  margin-bottom: 5px;
  border-radius: 5px;
}

#custom-filler {
  color: #6e738d;
  padding-left: 0px;
  padding-right: 9px;
  margin-top: 5px;
  margin-bottom: 5px;
  border-radius: 5px;
}

#custom-filler2 {
  color: #6e738d;
  padding-left: 3px;
  padding-right: 9px;
  margin-top: 5px;
  margin-bottom: 5px;
  border-radius: 5px;
}

#clock {
  padding-left: 10px;
  padding-right: 10px;
  font-size: 13px;
  font-weight: normal;
}

