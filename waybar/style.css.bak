#custom-pacman-update-icon,
#custom-notification,
#battery,
#pulseaudio,
#backlight {
  font-size: 18px;
  /* padding: 0px 5px 0px 5px; */
}

#backlight {
  color: #f7c67f;
  /* padding: 0px 15px 0px 5px; */
}

#box {
  background-color: #3C3E4E;
  padding-left: 10px;
  padding-right: 10px;
  margin-top: 5px;
  margin-bottom: 5px;
  border-radius: 10px;
  border: 2.5px solid #45495D;

}

#mpris {
  color: #ebfafa;
  animation: repeat;
  animation-name: blink;
  animation-duration: 3s;
  animation-timing-function: linear;
  animation-iteration-count: infinite;
  animation-direction: alternate;
  font-size: 15px;
  font-weight: normal;
  background: rgba(0, 0, 0, 0.8);
  padding-left: 10px;
  padding-right: 10px;
  margin: 5px;
  border-radius: 7px;
}

@keyframes blink {
  to {
    color: #262827;
  }
}

